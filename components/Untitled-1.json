{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "cache": {
      "images": {
        "maxAge": "31536000",
        "staleWhileRevalidate": "86400"
      }
    },
    "env": {
      "NODE_ENV": "production"
    }
  }